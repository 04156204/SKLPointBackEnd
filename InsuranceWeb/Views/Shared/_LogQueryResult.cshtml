@model List<InsuranceWeb.Model.InsurePointLog>



@if (Model == null)
{

}
else
{

    <table class="table">
        <thead>
            <tr>
                <th scope="col">流水號</th>
                <th scope="col">客戶ID</th>
                <th scope="col">點數動作</th>
                <th scope="col">異動點數數量</th>
                <th scope="col">記錄日期</th>
                <th scope="col">備註</th>
            </tr>
        </thead>
        <tbody>
            @if (Model == null)
            {

            }
            else
            {

                foreach (var pointLog in Model)
                {
                    <tr>
                        <th scope="row">@pointLog.Id</th>
                        <td>@pointLog.ApplicantId</td>
                        <td>@pointLog.Content</td>
                        <td>@pointLog.Point</td>
                        <td>@pointLog.CreateDate</td>
                        <td>@pointLog.Memo</td>

                    </tr>
                }
            }

        </tbody>
    </table>

}